Option Compare Database
Option Explicit

Public Function IsNothing(ByVal TheObject As Object) As Boolean
  IsNothing = (TheObject Is Nothing)
End Function

Public Function IsSomething(ByVal TheObject As Object) As Boolean
  IsSomething = Not (TheObject Is Nothing)
End Function