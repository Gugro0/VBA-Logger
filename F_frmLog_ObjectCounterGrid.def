Private Sub ctlActive_AfterUpdate()
  Select Case Nz(Me.ctlActive)
    Case 0: Me.StopLog
            Me.ctlActive.BackColor = vbRed
    Case 1: Me.StartLog
            Me.ctlActive.BackColor = vbGreen
  End Select
End Sub

