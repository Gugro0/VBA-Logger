  If IsNullString(Nz(Me.ctlProcedure)) Then
    Me.ctlProcedureComparer = vbNullString
  ElseIf IsNullString(Nz(Me.ctlProcedureComparer)) Then
    Me.ctlProcedureComparer = "="
  End If
  SetProcedureFilter
